{
    "name": "cypresslab/patch-manager-bundle",
    "description": "Symfony2 bundle to handle patch requests",
    "keywords": ["symfony2", "bundle", "rest", "patch", "api"],
    "type": "symfony-bundle",
    "require": {
        "php": ">=5.3.3",
        "symfony/property-access": "~2.5.6",
        "symfony/http-foundation": "~2.5.6",
        "phpcollection/phpcollection": "~0.4"
    },
    "require-dev": {
        "phpunit/phpunit": "~4.3",
        "mockery/mockery": "~0.9"
    },
    "autoload": {
        "psr-4": {
            "PatchManager\\": "src/PatchManager/",
            "PatchManager\\Tests\\": "tests/PatchManager/"
        }
    },
    "license": "LGPL-3.0+",
    "authors": [
        {
            "name": "Matteo Giachino",
            "email": "matteog@gmail.com"
        }
    ],
    "config": {
        "bin-dir":"bin"
    },
    "minimum-stability": "stable",
    "extra": {
        "branch-alias": {
            "dev-master": "1.0.x-dev"
        }
    },
    "suggest": {
        "finite/finite": "PatchManager with op 'sm'"
    }
}
